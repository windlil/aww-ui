{
  "name": "aww-ui",
  "type": "module",
  "version": "1.0.0",
  "description": "Vue components library",
  "author": "windlil",
  "license": "ISC",
  "keywords": [
    "UI",
    "vue",
    "vue-components"
  ],
  "main": "dist/lib/index.js",
  "types": "dist/es/index.d.ts",
  "module": "dist/es/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "prepare": "husky install",
    "build:awwui": "gulp -f packages/script/build/index.ts",
    "build": "vite build",
    "docs:dev": "pnpm run register:components && vitepress dev docs",
    "docs:build": "pnpm run register:components && vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "register:components": "vitepress-rc"
  },
  "dependencies": {
    "vue": "^3.1.0"
  },
  "packageManager": "pnpm@8.6.6",
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.8",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@types/gulp": "^4.0.13",
    "@types/gulp-autoprefixer": "^0.0.34",
    "@types/gulp-sass": "^5.0.1",
    "@types/node": "^20.4.5",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/runtime-core": "^3.3.4",
    "eslint": "^8.46.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-sass": "^5.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "sass": "^1.64.2",
    "sucrase": "^3.34.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vite-plugin-dts": "^3.4.0",
    "vitepress": "1.0.0-beta.7",
    "vitepress-theme-demoblock": "^3.0.3",
    "vitest": "^0.33.0"
  },
  "lint-staged": {
    "*.{js,ts,vue,tsx,jsx}": "eslint --fix"
  }
}
